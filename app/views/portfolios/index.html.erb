<h1>Portfolios items</h1>
<%= link_to "Create new", new_portfolio_url %>
<% @portfolios_items.each do |portfolios_items| %>
<p><%= portfolios_items.title %></p>
<p><%= portfolios_items.subtitle %></p>
<p><%= portfolios_items.body %></p>
<%= image_tag portfolios_items.thumb_image unless portfolios_items.thumb_image.nil? %>
<%= link_to "Edit portfolio", edit_portfolio_path(portfolios_items.id) %>
<% end %>
